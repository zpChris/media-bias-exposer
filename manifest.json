{
  "manifest_version": 2,
  "name": "Media Bias Exposer",
  "description": "This extension reveals the bias of various news sources. Go to https://openlensresearch.wordpress.com/ for more information.",
  "version": "1.0",
  "icons": {
    "128": "images/media-bias-exposer-128.png"
  },
  "browser_action": {
    "default_icon": "images/media-bias-exposer-128.png",
    "default_popup": "popup.html"
  },
  "background": {
    "page": "background.html"
  },
  "permissions": [
    "tabs"
  ],
  "content_security_policy":"script-src 'self' https://www.gstatic.com/ https://*.firebaseio.com https://www.googleapis.com; object-src 'self'"
}
